med />\n    <v-edu />\n    <v-skill />\n    <v-hobby />\n    <v-portofolio />\n\n    <div class=\"menu\">\n      <div class=\"menu-left\"></div>\n      <!-- <div class=\"menu-mode\">Dark</div> -->\n      <div class=\"men-right\"></div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport Personal from \"./components/personal\";\nimport Sosmed from \"./components/sosmed\";\nimport Education from \"./components/education\";\nimport Skill from \"./components/skill\";\nimport Hobby from \"./components/hobby\";\nimport Portofolio from \"./components/portofolio\";\n\nexport default {\n  name: \"App\",\n  components: {\n    \"v-personal\": Personal,\n    \"v-sosmed\": Sosmed,\n    \"v-edu\": Education,\n    \"v-skill\": Skill,\n    \"v-hobby\": Hobby,\n    \"v-portofolio\": Portofolio,\n  },\n  data() {\n    return {\n      mode: \"\",\n    };\n  },\n  methods: {\n    checkMode(theme) {\n      this.mode = theme;\n      document.documentElement.setAttribute(\"data-theme\", theme);\n    },\n  },\n};\n</script>\n\n<style lang=\"scss\">\n.resume {\n  padding-top: 30px !important;\n}\n\n#app {\n  color: var(--text-primary-color);\n  margin: 0;\n}\n\n.menu {\n  position: fixed;\n  z-index: 9999;\n  bottom: 0;\n  width: 100%;\n  // height: 50px;\n  // border-radius: 30px 30px 10px 10px;\n  // background: #fff;\n  // box-shadow: 0px 5px 10px 0px #2c2b2b;\n  // -webkit-box-shadow: 0px 5px 10px 0px #2c2b2b;\n  // -moz-box-shadow: 0px 5px 10px 0px #2c2b2b;\n}\n\n// .menu-mode {\n//   position: absolute;\n//   background: red;\n//   border-radius: 40px;\n//   left: 50%;\n//   margin-top: -30px;\n//   width: 60px;\n//   height: 60px;\n//   transform: translateX(-50%);\n// }\n</style>\n"]},"metadata":{},"sourceType":"module"}