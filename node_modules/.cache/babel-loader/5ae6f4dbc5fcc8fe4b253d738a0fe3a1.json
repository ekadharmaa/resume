\n  data() {\r\n    return {\r\n      desc: {\r\n        type: \"Email\",\r\n        name: \"Hafidz.bdt@gmail.com\",\r\n        link: \"mailto:hafidz.bdt@all-inedu.com\",\r\n        button: \"Send a Email\",\r\n      },\r\n      tabIcon: \"email\",\r\n    };\r\n  },\r\n  methods: {\r\n    checkTab(n) {\r\n      this.tabIcon = n;\r\n      if (n == \"email\") {\r\n        this.desc.type = \"Email\";\r\n        this.desc.name = \"Hafidz.bdt@gmail.com\";\r\n        this.desc.link = \"mailto:hafidz.bdt@all-inedu.com\";\r\n        this.desc.button = \"Send a Email\";\r\n      } else if (n == \"ig\") {\r\n        this.desc.type = \"Instagram\";\r\n        this.desc.name = \"Hafidz Annur\";\r\n        this.desc.link = \"https://www.instagram.com/hafidz_annur/\";\r\n        this.desc.button = \"Follow\";\r\n      }\r\n    },\r\n  },\r\n  created() {},\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\">\r\n:root {\r\n  --bg-sosmed: #e3dcdc;\r\n  --card-bg-sosmed: #3f4486;\r\n  --card-bg-sosmed-active: #df8f3a;\r\n  --card-color-sosmed: #fff;\r\n}\r\n\r\n:root[data-theme=\"dark\"] {\r\n  --bg-sosmed: #1f1835;\r\n  --card-bg-sosmed: rgb(234, 234, 255);\r\n  --card-bg-sosmed-active: #fff39b;\r\n  --card-color-sosmed: rgb(36, 101, 199);\r\n}\r\n\r\n#sosmed {\r\n  background: var(--bg-sosmed);\r\n  margin-top: -50px;\r\n  padding: 80px 20px 30px 20px;\r\n  border-radius: 40px 40px 0 0;\r\n}\r\n\r\n.sosmed-card label {\r\n  font-size: 0.8em;\r\n}\r\n\r\n.sosmed-name {\r\n  font-size: 1.1em;\r\n}\r\n\r\n.icon {\r\n  width: 100%;\r\n  background: var(--card-bg-sosmed);\r\n  color: var(--card-color-sosmed);\r\n  box-shadow: var(--card-shadow);\r\n  padding: 7px 10px;\r\n  text-align: center;\r\n  border-radius: 10px;\r\n  font-size: 1.3em;\r\n  transition: all 0.2s ease-in-out;\r\n  cursor: pointer;\r\n}\r\n\r\n.icon.active {\r\n  transform: scale(1.1);\r\n  background: var(--card-bg-sosmed-active);\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module"}