{"ast":null,"code":"export default {\n  name: \"socialMedia\",\n\n  data() {\n    return {\n      desc: {\n        type: \"Email\",\n        name: \"Hafidz.bdt@gmail.com\",\n        link: \"mailto:hafidz.bdt@all-inedu.com\",\n        button: \"Send a Email\"\n      },\n      tabIcon: \"email\"\n    };\n  },\n\n  methods: {\n    checkTab(n) {\n      this.tabIcon = n;\n\n      if (n == \"email\") {\n        this.desc.type = \"Email\";\n        this.desc.name = \"Hafidz.bdt@gmail.com\";\n        this.desc.link = \"mailto:hafidz.bdt@all-inedu.com\";\n        this.desc.button = \"Send a Email\";\n      } else if (n == \"ig\") {\n        this.desc.type = \"Instagram\";\n        this.desc.name = \"Hafidz Annur\";\n        this.desc.link = \"https://www.instagram.com/hafidz_annur/\";\n        this.desc.button = \"Follow\";\n      }\n    }\n\n  },\n\n  created() {}\n\n};","map":{"version":3,"mappings":"AAqDA,eAAe;EACbA,IAAI,EAAE,aADO;;EAEbC,IAAI,GAAG;IACL,OAAO;MACLC,IAAI,EAAE;QACJC,IAAI,EAAE,OADF;QAEJH,IAAI,EAAE,sBAFF;QAGJI,IAAI,EAAE,iCAHF;QAIJC,MAAM,EAAE;MAJJ,CADD;MAOLC,OAAO,EAAE;IAPJ,CAAP;EASD,CAZY;;EAabC,OAAO,EAAE;IACPC,QAAQ,CAACC,CAAD,EAAI;MACV,KAAKH,OAAL,GAAeG,CAAf;;MACA,IAAIA,KAAK,OAAT,EAAkB;QAChB,KAAKP,IAAL,CAAUC,IAAV,GAAiB,OAAjB;QACA,KAAKD,IAAL,CAAUF,IAAV,GAAiB,sBAAjB;QACA,KAAKE,IAAL,CAAUE,IAAV,GAAiB,iCAAjB;QACA,KAAKF,IAAL,CAAUG,MAAV,GAAmB,cAAnB;MACF,CALA,MAKO,IAAII,KAAK,IAAT,EAAe;QACpB,KAAKP,IAAL,CAAUC,IAAV,GAAiB,WAAjB;QACA,KAAKD,IAAL,CAAUF,IAAV,GAAiB,cAAjB;QACA,KAAKE,IAAL,CAAUE,IAAV,GAAiB,yCAAjB;QACA,KAAKF,IAAL,CAAUG,MAAV,GAAmB,QAAnB;MACF;IACD;;EAdM,CAbI;;EA6BbK,OAAO,GAAG,CAAE;;AA7BC,CAAf","names":["name","data","desc","type","link","button","tabIcon","methods","checkTab","n","created"],"sourceRoot":"","sources":["C:\\Users\\dl6\\Documents\\Hafidz\\resume\\src\\components\\sosmed.vue"],"sourcesContent":["<template>\r\n  <div id=\"sosmed\">\r\n    <div class=\"title text-end\">Social Media</div>\r\n    <div class=\"row align-items-center g-2\">\r\n      <div class=\