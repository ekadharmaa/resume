-flex\" v-if=\"tab == 'all' || tab == 'tools'\">\r\n          <div class=\"skill-icon align-items-center\">\r\n            <img src=\"@/assets/img/icon/tools/photoshop-cc-4.svg\" alt=\"\" />\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"skillComp\",\r\n  data() {\r\n    return {\r\n      tab: \"all\",\r\n      skill: {\r\n        backend: [\r\n          \"back-end/laravel-2.svg\",\r\n          \"back-end/codeigniter.svg\",\r\n          \"back-end/php-1.svg\",\r\n          \"back-end/firebase-1.svg\",\r\n          \"back-end/mysql-4.svg\",\r\n        ],\r\n        frontend: [\r\n          \"front-end/html-1.svg\",\r\n          \"front-end/css-3.svg\",\r\n          \"front-end/javascript-1.svg\",\r\n          \"front-end/jquery.svg\",\r\n          \"front-end/sass-1.svg\",\r\n          \"front-end/bootstrap-5-1.svg\",\r\n          \"front-end/vue-9.svg\",\r\n        ],\r\n      },\r\n    };\r\n  },\r\n  created() {},\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\">\r\nhtml {\r\n  --skill-bg: #f9dad5;\r\n  --skill-color: #000;\r\n  --skill-bg-icon: #fff;\r\n  --skill-btn: #11184f;\r\n  --skill-btn-color: #fff;\r\n  --skill-btn-active: #ea7a66;\r\n  --skill-btn-color-active: #fff;\r\n}\r\n\r\nhtml[data-theme=\"dark\"] {\r\n  --skill-bg: #bbbbbb3a;\r\n  --skill-color: #fff;\r\n  --skill-bg-icon: rgb(207, 207, 207);\r\n  --skill-btn: #6474e5;\r\n  --skill-btn-color: #fff;\r\n  --skill-btn-active: #dddcdc;\r\n  --skill-btn-color-active: #000;\r\n}\r\n\r\n#skill {\r\n  margin-top: -30px;\r\n  padding: 50px 20px 50px 20px;\r\n  background: var(--skill-bg);\r\n  color: var(--skill-color);\r\n  z-index: 0;\r\n}\r\n\r\n.tab {\r\n  position: relative;\r\n  width: 100%;\r\n  padding: 10px 0;\r\n  height: auto;\r\n  overflow: auto;\r\n  display: flex;\r\n}\r\n\r\n.tab-item {\r\n  background: var(--skill-btn);\r\n  color: var(--skill-btn-color);\r\n  padding: 5px 20px;\r\n  margin-right: 8px;\r\n  display: inline;\r\n  white-space: nowrap;\r\n  border-radius: 40px;\r\n  font-size: 0.9em;\r\n  cursor: pointer;\r\n  transition: all 0.3s ease-in-out;\r\n}\r\n\r\n.tab-item.active {\r\n  background: var(--skill-btn-active);\r\n  color: var(--skill-btn-color-active);\r\n}\r\n\r\n.skill-card {\r\n  width: 20% !important;\r\n  margin-right: 10px;\r\n}\r\n\r\n.skill-icon {\r\n  padding: 0px 10px;\r\n  background: var(--skill-bg-icon);\r\n  border-radius: 20px;\r\n  min-height: 80px;\r\n  display: flex;\r\n}\r\n\r\n.skill-icon img {\r\n  width: 100%;\r\n  align-items: center;\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module"}