             class=\"form-control\"\r\n                placeholder=\"Full Name\"\r\n                required\r\n              />\r\n              <label for=\"email\">Email</label>\r\n            </div>\r\n            <textarea\r\n              name=\"\"\r\n              id=\"\"\r\n              cols=\"30\"\r\n              rows=\"5\"\r\n              class=\"form-control\"\r\n              v-model=\"contact.message\"\r\n              required\r\n            ></textarea>\r\n            <hr />\r\n            <div class=\"text-end\">\r\n              <button type=\"submit\" class=\"btn btn-primary\">Submit</button>\r\n            </div>\r\n          </form>\r\n        </div>\r\n      </div>\r\n      <div\r\n        class=\"row mb-3\"\r\n        v-if=\"this.$route.params.id == 'list' && messageList?.length != 0\"\r\n      >\r\n        <div class=\"col-12\">\r\n          <h6>Message List</h6>\r\n          <div class=\"list-group\">\r\n            <div class=\"list-group-item\" v-for=\"i in messageList\" :key=\"i\">\r\n              {{ i.name }}\r\n            </div>\r\n          </div>\r\n          <div class=\"d-grid gap-2\">\r\n            <button\r\n              class=\"btn btn-outline-danger\"\r\n              type=\"button\"\r\n              @click=\"removeALL\"\r\n            >\r\n              Remove ALL\r\n            </button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Services from \"../Services\";\r\nimport Swal from \"sweetalert2\";\r\n\r\nexport default {\r\n  name: \"contactUs\",\r\n  props: {\r\n    messageList: Object,\r\n  },\r\n  data() {\r\n    return {\r\n      contact: {\r\n        name: \"\",\r\n        email: \"\",\r\n        message: \"\",\r\n      },\r\n      contact_list: [],\r\n    };\r\n  },\r\n  methods: {\r\n    toast(s, t) {\r\n      const Toast = Swal.mixin({\r\n        toast: true,\r\n        position: \"bottom-end\",\r\n        showConfirmButton: false,\r\n        timer: 3000,\r\n        timerProgressBar: true,\r\n        didOpen: (toast) => {\r\n          toast.addEventListener(\"mouseenter\", Swal.stopTimer);\r\n          toast.addEventListener(\"mouseleave\", Swal.resumeTimer);\r\n        },\r\n      });\r\n\r\n      Toast.fire({\r\n        icon: s,\r\n        title: t,\r\n      });\r\n    },\r\n\r\n    handleSubmit() {\r\n      Swal.showLoading();\r\n      Services.create(this.contact)\r\n        .then(() => {\r\n          this.contact = {\r\n            name: \"\",\r\n            email: \"\",\r\n            message: \"\",\r\n          };\r\n          this.toast(\"success\", \"Message sent successfully!\");\r\n        })\r\n        .catch((e) => {\r\n          console.log(e);\r\n          Swal.close();\r\n        });\r\n      this.$emit(\"check\", null);\r\n    },\r\n\r\n    removeALL() {\r\n      Services.deleteAll();\r\n      this.$emit(\"check\", null);\r\n    },\r\n  },\r\n  created() {},\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n#contact {\r\n  background: var(--color-6);\r\n}\r\n\r\n.form-control {\r\n  box-shadow: none !important;\r\n  border: 1px solid #dedede;\r\n}\r\n\r\n.form-control:focus {\r\n  border: 1.5px solid var(--color-1);\r\n}\r\n\r\n.form-control::placeholder {\r\n  color: transparent !important;\r\n}\r\n\r\n.form-control:valid {\r\n  border: 1.5px solid var(--color-5);\r\n}\r\n\r\n.form-group {\r\n  position: relative;\r\n  padding-top: 5px;\r\n}\r\n\r\n.form-group label {\r\n  position: absolute;\r\n  top: 12px;\r\n  margin-left: 12px;\r\n}\r\n\r\ninput:focus + label,\r\ninput:active + label {\r\n  position: absolute !important;\r\n  top: -15px !important;\r\n  margin-left: 12px !important;\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module"}